{
  "name": "geo-sot/laravel-env-editor",
  "description": "A laravel Package that Supports .Env File editing and backup ",
  "keywords": [
    "geo-sot",
    "laravel",
    "laravel-env-editor",
    "EnvEditor"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Geo Sot",
      "email": "geo.sotis@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "laravel/framework": "5.5.x"
  },
  "autoload": {
    "psr-4": {
      "GeoSot\\EnvEditor\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.*-dev"
    },
    "laravel": {
      "providers": [
        "GeoSot\\EnvEditor\\ServiceProvider"
      ],
      "aliases": {
        "EnvEditor": "GeoSot\\EnvEditor\\Facades\\EnvEditor"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "GeoSot\\EnvEditor\\": "tests/"
    }
  }
}




